{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [ {
      "source" : "/login",
      "destination" : "https://poller-eefdf.firebaseapp.com/login",
      "type" : 301
    }, {
      "source" : "/*",
      "destination" : "https://poller-eefdf.firebaseapp.com",
      "type" : 302
    } ]
  }
}
